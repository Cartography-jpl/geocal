#! /usr/bin/env python
#
# This short program prints out the keys found in a SQLiteShelf.
from afids import SQLiteShelf
from optparse import OptionParser

parser = OptionParser(version="%prog August 13, 2012",
usage="""usage: %prog [-h] file

This prints out the keys found in a SQLiteShelf.
""")

# Parse command line arguments
(options, args) = parser.parse_args()
if(len(args) != 1):
    parser.error("Need to specify a SQLiteShelf file")

t = SQLiteShelf(args[0], "r").keys()
t.sort
for v in t:
    print v

